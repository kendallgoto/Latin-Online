RewriteEngine On
RewriteRule ^wordlist/([0-9]+)/([0-9]+)/([a-z]+)$ wordlist.php?start=$1&end=$2&method=$3
RewriteRule ^wordlist/([0-9]+)/([0-9]+)/([a-z]+)/$ wordlist.php?start=$1&end=$2&method=$3

RewriteRule ^pp/([0-9]+)/([0-9]+)/([a-z]+)$ principalparts.php?start=$1&end=$2&pp=$3
RewriteRule ^pp/([0-9]+)/([0-9]+)/([a-z]+)/$ principalparts.php?start=$1&end=$2&pp=$3

RewriteRule ^vocab/([0-9]+)/([0-9]+)/([a-z0-9]+)$ vocab.php?start=$1&end=$2&dir=$3
RewriteRule ^vocab/([0-9]+)/([0-9]+)/([a-z0-9]+)/$ vocab.php?start=$1&end=$2&dir=$3

RewriteRule ^num/([a-z0-9]+)$ numbers.php?dir=$1
RewriteRule ^num/([a-z0-9]+)/$ numbers.php?dir=$1

RewriteRule ^gensg/([0-9]+)/([0-9]+)$ gensg.php?start=$1&end=$2
RewriteRule ^gensg/([0-9]+)/([0-9]+)/$ gensg.php?start=$1&end=$2

RewriteRule ^dict$ whitakers.php
RewriteRule ^dict/$ whitakers.php
